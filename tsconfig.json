{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@hx/components/*": ["src/components/*"],
      "@hx/content/*": ["src/content/*"],
      "@hx/layouts/*": ["src/layouts/*"],
      "@hx/styles/*": ["src/styles/*"],
      "@hx/consts": ["src/consts.ts"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [".astro/types.d.ts", ".astro/content.d.ts", "**/*"],
  "exclude": ["dist"]
}
